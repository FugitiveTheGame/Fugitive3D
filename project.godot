[application]

config/name="Fugitive 3D"
run/main_scene="res://entry/Entry.tscn"
boot_splash/image="res://splash.png"
config/icon="res://icon.png"
config/quit_on_go_back=false

[audio]

enable_audio_input=true

[autoload]

ClientNetwork="*res://networking/ClientNetwork.gd"
ServerNetwork="*res://networking/ServerNetwork.gd"
GameData="*res://networking/GameData.gd"
vr="*res://addons/OQ_Toolkit/vr_autoload.gd"
UserData="*res://common/UserData.gd"
Maps="*res://common/game/maps/MapDirectory.gd"
GameAnalytics="*res://addons/Godot-GameAnalytics/GameAnalytics.gd"
Feedback="*res://feedback/Feedback.gd"

[debug]

gdscript/warnings/unused_argument=false
gdscript/warnings/return_value_discarded=false

[display]

window/size/width=1280
window/size/height=720
window/size/fullscreen=true
window/size/test_width=1280
window/size/test_height=720
window/dpi/allow_hidpi=true
window/stretch/mode="2d"
window/stretch/aspect="expand"
window/stretch/mode.mobile="2d"
window/stretch/aspect.mobile="keep_height"
window/size/height.OSX=900
window/size/width.X11=1600
window/size/height.X11=900
window/size/fullscreen.server=false
window/size/width.Windows=1600
window/size/height.Windows=900
window/size/fullscreen.debug=false
window/size/width.OSX=720

[editor_plugins]

enabled=PoolStringArray( "Fugitive3D", "Godot-GameAnalytics", "LANServerBroadcast", "opus" )

[gdnative]

singletons=[ "res://addons/godot-oculus/godot_oculus.gdnlib", "res://addons/godot-openvr/godot_openvr.gdnlib", "res://addons/godot_ovrmobile/godot_ovrmobile.gdnlib" ]
